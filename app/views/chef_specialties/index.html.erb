<p id="notice"><%= notice %></p>

<h1>Listing Chef Specialties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chef_specialties.each do |chef_specialty| %>
      <tr>
        <td><%= link_to 'Show', chef_specialty %></td>
        <td><%= link_to 'Edit', edit_chef_specialty_path(chef_specialty) %></td>
        <td><%= link_to 'Destroy', chef_specialty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chef specialty', new_chef_specialty_path %>
